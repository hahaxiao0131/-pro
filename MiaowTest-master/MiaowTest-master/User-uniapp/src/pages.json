{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
			"^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
			"^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
			"^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue"
		}
	},
	"pages": [
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "考神附体Pro",
				"navigationBarTextStyle": "white",
				"navigationBarBackgroundColor": "#007AFF",
				"h5": {
					"style": {
						"navigationBarTextStyle": "white"
					}
				},
				"enablePullDownRefresh": true,
				"backgroundColor": "#F5F7FA"
			}
		},
		{
			"path": "pages/my/my",
			"style": {
				"navigationBarTitleText": "我的",
				"navigationStyle": "custom",
				"disableScroll": false,
				"enablePullDownRefresh": false,
				"backgroundColor": "#F8FDFF",
				"h5": {
					"style": {
						"navigationBarTextStyle": "black"
					}
				}
			}
		},
		{
			"path": "pages/exam/exam",
			"style": {
				"navigationBarTitleText": "所有考试",
				"navigationStyle": "custom",
				"disableScroll": false,
				"enablePullDownRefresh": true,
				"backgroundColor": "#F8FDFF",
				"navigationBarTextStyle": "black",
				"app-plus": {
					"bounce": "none",
					"pullToRefresh": {
						"support": true,
						"style": "circle",
						"color": "#007AFF"
					}
				},
				"h5": {
					"style": {
						"navigationBarTextStyle": "black"
					}
				}
			}
		},
		{
			"path": "pages/tools/tools",
			"style": {
				"navigationBarTitleText": "所有工具",
				"navigationStyle": "custom",
				"disableScroll": false,
				"navigationBarTextStyle": "black",
				"h5": {
					"style": {
						"navigationBarTextStyle": "black"
					}
				}
			}
		},
		{
			"path": "pages/public/searchview",
			"style": {
				"navigationBarTitleText": "搜索",
				"navigationBarTextStyle": "black"
			}
		},
		{
			"path": "pages/exam/crquestionbankView",
			"style": {
				"navigationBarTitleText": "创建题库",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/exam/subjectdetailview",
			"style": {
				"navigationBarTitleText": "考试详情"
			}
		},
		{
			"path": "pages/exam/PracticeSettingView",
			"style": {
				"navigationBarTitleText": "练习设置"
			}
		},
		{
			"path": "pages/exam/PracticeView",
			"style": {
				"navigationBarTitleText": "练习",
				"navigationStyle": "custom",
				"navigationBarTextStyle": "black"
			}
		},
		{
			"path": "pages/exam/PracticeResultsView",
			"style": {
				"navigationBarTitleText": "练习结果"
			}
		},
		{
			"path": "pages/my/UserInfoView",
			"style": {
				"navigationBarTitleText": "个人信息",
				"disableScroll": true
			}
		},
		{
			"path": "pages/my/UserRegisterView",
			"style": {
				"navigationBarTitleText": "注册"
			}
		},
		{
			"path": "pages/my/UserLoginView",
			"style": {
				"navigationBarTitleText": "登录",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/exam/ManualImportView",
			"style": {
				"navigationBarTitleText": "手动导入"
			}
		},
		{
			"path": "pages/exam/UserBankView",
			"style": {
				"navigationBarTitleText": "题库详情",
				"disableScroll": true
			}
		},
		{
			"path": "pages/exam/WrongQuestionView",
			"style": {
				"navigationBarTitleText": "我的错题"
			}
		},
		{
			"path": "pages/my/MyFavoriteView",
			"style": {
				"navigationBarTitleText": "我的收藏",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/my/MyWrongView",
			"style": {
				"navigationBarTitleText": "我的错题"
			}
		},
		{
			"path": "pages/tools/TimerToolView",
			"style": {
				"navigationBarTitleText": "计时器",
				"disableScroll": false,
				"navigationBarTextStyle": "black"
			}
		},
		{
			"path": "pages/public/feedbackview",
			"style": {
				"navigationBarTitleText": "意见反馈"
			}
		},
		{
			"path": "pages/exam/ExamDiskView",
			"style": {
				"navigationBarTitleText": "资料详情",
				"disableScroll": true
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "考神附体Pro",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"app-plus": {
			"titleView": false
		}
	},

	"h5": {
		"style": {
			"display": "none"
		},
		"mediaQuery": {
			"minWidth": "1024px",
			"style": {
				"display": "none"
			}
		}
	}
}

